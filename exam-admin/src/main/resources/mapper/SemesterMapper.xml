<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sias.admin.mapper.SemesterMapper">

    <resultMap id="BaseResultMap" type="com.sias.admin.domain.Semester">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="deleted" column="deleted" jdbcType="BIT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,deleted
    </sql>
    <select id="selectByPrimaryKey" resultType="com.sias.admin.domain.Semester">
        select <include refid="Base_Column_List"/> from semester where id = #{id}
    </select>
    <select id="list" resultType="com.sias.admin.domain.Semester">
        select <include refid="Base_Column_List"/> from semester
    </select>
</mapper>
